<odoo>

  <template id="assets_frontend" name="CMS toolbar frontend assets" inherit_id="website.assets_frontend">

    <xpath expr="//link[last()]" position="after">
      <link rel="stylesheet" type="text/less" href="/cms_toolbar/static/src/less/toolbar.less"/>
    </xpath>
    <xpath expr="//script[last()]" position="after">
      <script type="text/javascript" src="/cms_toolbar/static/src/js/tooltips.js"></script>
      <script type="text/javascript" src="/cms_toolbar/static/src/js/publish.js"></script>
    </xpath>

  </template>

</odoo>
