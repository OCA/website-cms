<odoo>
    <template
        id="assets_frontend"
        name="CMS Form frontend assets"
        inherit_id="website.assets_frontend"
    >
        <xpath expr="//link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/cms_form/static/src/scss/cms_form.scss"
            />
            <link
                rel="stylesheet"
                type="text/scss"
                href="/cms_form/static/src/scss/progressbar.scss"
            />
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script
                type="text/javascript"
                src="/cms_form/static/src/js/select2widgets.js"
            />
            <script
                type="text/javascript"
                src="/cms_form/static/src/js/date_widget.js"
            />
            <script
                type="text/javascript"
                src="/cms_form/static/src/js/textarea_widget.js"
            />
            <script
                type="text/javascript"
                src="/cms_form/static/src/js/master_slave.js"
            />
            <script
                type="text/javascript"
                src="/cms_form/static/src/js/lock_copy_paste.js"
            />
            <script type="text/javascript" src="/cms_form/static/src/js/ajax.js" />
        </xpath>
    </template>
</odoo>
